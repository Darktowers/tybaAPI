{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "A minimalist TypeScript-Express boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint . --ext .ts --fix",
    "postinstall": "chmod +x ./scripts/dev.sh",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "./scripts/dev.sh",
    "devWin": "sh ./scripts/dev.sh",
    "start": "node dist/server.js",
    "test": "jest",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/request": "^2.48.4",
    "bcrypt": "^5.0.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-jwt": "^5.3.3",
    "express-kun": "^1.0.24",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.9.10",
    "nodemon": "2.0.3",
    "request": "^2.88.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.1",
    "@types/jest": "25.2.1",
    "@types/mongoose": "5.7.12",
    "@types/node": "13.13.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.4.0",
    "jest-express": "1.12.0",
    "mockingoose": "2.13.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3"
  },
  "license": "MIT"
}
